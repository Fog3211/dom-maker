  "author": "Fog3211",
  "license": "MIT",
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@rollup/plugin-babel": "5.3.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.31",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "@babel/runtime": "^7.16.5"
  }
}
